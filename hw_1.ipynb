    "def getMolDescriptors(mol, missingVal=None):\n",
    "    \"\"\"calculate the full list of RDKit descriptors for a molecule\"\"\"\n",
    "    \"\"\"missingVal is used when descriptor cannot be calculated\"\"\"\n",
    "    res = {}\n",
    "    for nm,fn in Descriptors._descList:\n",
    "        try:\n",
    "            val = fn(mol)\n",
    "        except:\n",
    "            import traceback\n",
    "            traceback.print_exc()\n",
    "            val = missingVal\n",
    "        res[nm] = val\n",
    "    return res"
   ]
